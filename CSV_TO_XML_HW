import pandas as pd

# Načíst CSV data
data = pd.read_csv('https://www.hurtowniaprzemyslowa.pl/xml/hurtowniaprzemyslowa_csv.csv')
# Převést do XML
xml_data = data.to_xml()

# Uložit XML do souboru (tento krok můžete upravit podle potřeby)
with open('data.xml', 'w') as file:
    file.write(xml_data)
